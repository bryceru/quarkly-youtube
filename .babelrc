{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    "syntax-dynamic-import",
    [
      "transform-react-remove-prop-types",
      {
        "mode": "wrap"
      }
    ],
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "test": {
      "plugins": ["require-context-hook"]
    }
  }
}
